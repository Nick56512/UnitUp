<Application x:Class="GroupManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GroupManager"
             xmlns:models="clr-namespace:GroupManager.Models" 
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:MainBootsrapper x:Key="bootstrapper"></local:MainBootsrapper>
                </ResourceDictionary>
                <ResourceDictionary>
                    <models:IndexConverter x:Key="indexConverter"></models:IndexConverter>
                   
                </ResourceDictionary>
                <!--<ResourceDictionary>
                    <models:CalendarMonthConverter x:Key="MonthConverter"></models:CalendarMonthConverter>
                </ResourceDictionary>-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
